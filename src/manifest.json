{
    "manifest_version": 2,
    "name": "easycache",
    "description": "An interface to various web caches/archivers",
    "version": "0.0.1",
    "options_page": "options.html",
    "permissions": [
	"contextMenus",
	"activeTab",
	"https://www.bing.com/*"
    ],
    "background": {
	"persistent": false,
	"scripts": ["event_page.mjs"]
    },
    "browser_action": {
        "default_popup": "popup.html"
    },
    "icons": {
	"128": "icon128.png"
    }
}
