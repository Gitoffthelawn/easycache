{
    "manifest_version": 2,
    "name": "easycache",
    "description": "An interface to various web caches/archivers (Google, Bing, Wayback Machine, archive.is). You can add own providers.",
    "version": "0.0.2",
    "options_ui": {
	"page": "options.html"
    },
    "permissions": [
	"contextMenus",
	"activeTab",
	"https://www.bing.com/*"
    ],
    "background": {
	"persistent": false,
	"scripts": ["event_page.mjs"]
    },
    "browser_action": {
        "default_popup": "popup.html"
    },
    "icons": {
	"128": "icon128.png"
    }
}
